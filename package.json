{
  "name": "evolved-equator",
  "type": "module",
  "version": "0.2.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "png": "find public -type f -name \"*.png\" -exec pngquant --quality=65-80 --ext .png --force {} \\;",
    "jpeg": "find public -type f \\( -name \"*.jpg\" -o -name \"*.jpeg\" \\) -exec sh -c 'jpegtran -copy none -optimize -progressive \"$1\" > \"$1.tmp\" && mv \"$1.tmp\" \"$1\" || rm -f \"$1.tmp\"' sh {} \\;",
    "fix-extensions": "find public -name \"*.png\" -exec sh -c 'if file \"$1\" | grep -q JPEG; then mv \"$1\" \"${1%.png}.jpg\"; echo \"Fixed: $1 -> ${1%.png}.jpg\"; fi' sh {} \\; && find public \\( -name \"*.jpg\" -o -name \"*.jpeg\" \\) -exec sh -c 'if file \"$1\" | grep -q PNG; then mv \"$1\" \"${1%.*}.png\"; echo \"Fixed: $1 -> ${1%.*}.png\"; fi' sh {} \\;",
    "fix-image-paths": "find src/content/blog -name \"*.md\" -exec sed -i '' 's|](/|](/simple-blog/|g' {} \\;",
    "compress": "npm run fix-extensions && npm run png && npm run jpeg"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.6",
    "@astrojs/react": "^4.1.5",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/solid-js": "^5.0.3",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "astro": "^5.1.7",
    "clsx": "^2.1.1",
    "p5-react-component": "^0.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.16",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-prettier": "^5.2.3",
    "less": "^4.2.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "sass-embedded": "^1.83.4",
    "stylelint": "^16.13.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-prettier": "^5.0.2"
  }
}
